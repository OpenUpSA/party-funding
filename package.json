{
  "name": "party-funding",
  "version": "0.0.1",
  "description": "MVC Party Funding explorer tool",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html",
    "postbuild": "mkdir -p dist/data && cp data/out/* dist/data/",
    "webflow-import": "npx import-webflow webflow-export.zip",
    "lint": "eslint src/js/ --ext js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "data/out",
        "staticOutDir": "data"
      }
    ]
  },
  "importWebflowExport": {
    "copyTrees": {
      "css": "src/css",
      "js": "src/js",
      "images": "src/images"
    },
    "importHtml": [
      {
        "glob": "*.html",
        "destDir": "src",
        "transforms": "./src/js/webflow-transform.js"
      }
    ]
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "parcel": "^2.6.2",
    "parcel-plugin-static-files-copy": "^2.5.1"
  },
  "dependencies": {
    "d3": "^7.6.1",
    "d3-fetch": "^3.0.1",
    "d3-hierarchy": "^3.1.2",
    "d3-selection": "^3.0.0",
    "jquery": "^3.5.1",
    "js-worker-search": "github:ranihorev/js-worker-search"
  }
}
